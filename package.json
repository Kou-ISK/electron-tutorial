    "@types/video.js": "^7.3.52",
    "@types/videojs": "^5.16.2",
    "concurrently": "^8.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-localshortcut": "^3.2.1",
    "electron-reload": "^2.0.0-alpha.1",
    "electronmon": "^2.0.2",
    "eslint": "^8.49.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "minimatch": "^9.0.3",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-datepicker": "^4.16.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.1",
    "tsc": "^2.0.4",
    "typescript": "^4.9.5",
    "ulid": "^2.3.0",
